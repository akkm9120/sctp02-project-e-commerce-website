{
  "dev": {
    "driver": {
      "ENV": "DB_DRIVER"
    },
    "user": {
      "ENV": "DB_USER"
    },
    "password": {
      "ENV": "DB_PASSWORD"
    },
    "database": {
      "ENV": "DB_DATABASE"
    },
    "host": {
      "ENV": "DB_HOST"
    },
    "port": {
      "ENV": "DB_PORT"
    },
    "ssl": {
      "rejectUnauthorized": false
    }
  },
  "production": {
    "driver": "mysql2",
    "user": {
      "ENV": "MYSQLUSER"
    },
    "password": {
      "ENV": "MYSQLPASSWORD"
    },
    "database": {
      "ENV": "MYSQLDATABASE"
    },
    "host": {
      "ENV": "MYSQLHOST"
    },
    "port": {
      "ENV": "MYSQLPORT"
    },
    "ssl": {
      "rejectUnauthorized": false
    }
  }
}
